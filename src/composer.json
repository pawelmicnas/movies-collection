{
  "name": "pawelmicnas/movies-collection",
  "authors": [
    {
      "name": "Pawe≈Ç Micnas",
      "email": "pawelmicnas@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "symfony/console": "^7.1",
    "symfony/framework-bundle": "^7.1",
    "symfony/messenger": "^7.1",
    "symfony/property-access": "^7.1",
    "symfony/runtime": "^7.1",
    "symfony/serializer": "^7.1",
    "symfony/yaml": "^7.1"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  }
}
